#include "msm8996-mtp-for-somc.dtsi"
#include "msm8996-mdss-panels-for-somc.dtsi"

/ {
	aliases {
		uart2 = &blsp1_uart2;
		uart3 = &blsp1_uart3;
		i2c11 = &i2c_11;
		sdhc4 = &sdhc_4;
	};

	reserved-memory {
		debug_region: debug_region@83300000 {
			compatible = "removed-dma-pool", "qcom,debug_memory";
			no-map;
			reg = <0 0x83300000 0 0x100000>;
			label = "debug_mem";
		};

		pstore_reserve_mem: pstore_reserve_mem_region_region@83200000 {
			compatible = "removed-dma-pool";
			no-map;
			reg = <0 0x83200000 0 0x100000>;
		};
	};

	ramoops {
		compatible = "pstore-ramoops";
		memory-region = <&pstore_reserve_mem>;
	};
};

/* usb2s config */
&usb2s {
	status = "ok";
	qcom,psy-not-used;
};

&qusb_phy1 {
	/delete-property/ qcom,hold-reset;
};

&tspp {
	status = "disabled";
};



/* for bcmdhd */
/ {
	wlreg_on: wlreg_on {
		compatible = "regulator-fixed";
		regulator-name = "wlreg_on";
		gpio = <&tlmm 84 0>;
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		startup-delay-us = <100>;
		enable-active-high;
	};
	bcmdhd_wlan: bcmdhd_wlan {
		compatible = "android,bcmdhd_wlan";
		gpios = <&tlmm 79 0x00>; /* WL_HOST_WAKE */
		wlreg_on-supply = <&wlreg_on>;
	};
};

&soc {
	/* I2C : BLSP11 */
	i2c_11: i2c@75b9000 { /* BLSP2 QUP4 */
		compatible = "qcom,i2c-msm-v2";
		#address-cells = <1>;
		#size-cells = <0>;
		reg-names = "qup_phys_addr";
		reg = <0x75b9000 0x1000>;
		interrupt-names = "qup_irq";
		interrupts = <0 105 0>;
		dmas = <&dma_blsp2 20 64 0x20000020 0x20>,
			<&dma_blsp2 21 32 0x20000020 0x20>;
		dma-names = "tx", "rx";
		qcom,master-id = <84>;
		qcom,clk-freq-out = <400000>;
		qcom,clk-freq-in  = <19200000>;
		clock-names = "iface_clk", "core_clk";
		clocks = <&clock_gcc clk_gcc_blsp2_ahb_clk>,
				<&clock_gcc clk_gcc_blsp2_qup5_i2c_apps_clk>;
		pinctrl-names = "i2c_active", "i2c_sleep";
		pinctrl-0 = <&i2c_11_active>;
		pinctrl-1 = <&i2c_11_sleep>;
	};

	tlmm: pinctrl@01010000 {
		i2c_11 {
			i2c_11_active: i2c_11_active {
				mux {
					pins = "gpio60", "gpio61";
					function = "blsp_i2c11";
				};

				config {
					pins = "gpio60", "gpio61";
					drive-strength = <4>;
					bias-disable;
				};
			};

			i2c_11_sleep: i2c_11_sleep {
				mux {
					pins = "gpio60", "gpio61";
					function = "blsp_i2c11";
				};

				config {
					pins = "gpio60", "gpio61";
					drive-strength = <4>;
					bias-pull-up;
				};
			};
		};
	};

	/* I2C : BLSP12 */
	i2c@75ba000 {
		pinctrl-0 = <&msm_gpio_87_act &msm_gpio_88_act>;
		pinctrl-1 = <&msm_gpio_87_sus &msm_gpio_88_sus>;
		status = "disabled";

		/delete-node/ synaptics@20;
		synaptics_clearpad@2c {
			compatible = "synaptics,clearpad";
			reg = <0x2c>;
			interrupt-parent = <&tlmm>;
			interrupts = <125 0x2>;
			synaptics,irq_gpio = <&tlmm 125 0x00>;
			post_probe_start = <0>;
			synaptics,firmware_name = "touch_module_id_0x%02x.img";
			flash_on_post_probe = <1>;
			flip_config = <0>;
			watchdog_enabled = <1>;
			watchdog_delay_ms = <3000>;
			charger_supported = <0>;
			pen_supported = <0>;
			glove_supported = <1>;
			cover_supported = <1>;
			touch_pressure_enabled = <1>;
			touch_size_enabled = <0>;
			touch_orientation_enabled = <0>;
			preset_x_max = <1079>;
			preset_y_max = <1919>;
			preset_n_fingers = <10>;
			wakeup_gesture_supported = <1>;
			wakeup_gesture_lpm_disabled = <1>;
			wakeup_gesture_timeout = <0>;
			wakeup_gesture_use_workaround_for_felica = <1>;
			chip_id = <0x38>;
			wakeup_gesture {
				double_tap {
					gesture_code = <0x0003>;
					event_00 {
						type = <2>; /* LOG */
						message = "=== DOUBLE TAP ===";
					};
					event_01 {
						type = <1>; /* KEY */
						code = <531>; /* TOUCHPAD_ON */
						down = <1>;
					};
					event_02 {
						type = <1>; /* KEY */
						code = <531>; /* TOUCHPAD_ON */
						down = <0>;
					};
					event_03 {
						type = <99>; /* END */
					};
				};
			};
			/* chip settings */
			clearpad_default {
				flash_default_timeout_ms = <20000>;
				calibrate_on_fwflash = <0>;
				calibration_supported = <0>;
				hwreset_delay_for_powerup_ms = <60>;
				interrupt_default_wait_ms = <1000>;
			};
			S3330 {
				flash_default_timeout_ms = <20000>;
				calibrate_on_fwflash = <0>;
				calibration_supported = <0>;
				hwreset_delay_for_powerup_ms = <60>;
				interrupt_default_wait_ms = <1000>;
			};
			S332U {
				flash_default_timeout_ms = <20000>;
				calibration_supported = <1>;
				hwreset_delay_for_powerup_ms = <60>;
				interrupt_default_wait_ms = <1000>;
				calibrate_on_fwflash = <0>;
			};
		};
	};

	gpio_keys {
		compatible = "gpio-keys";
		input-name = "gpio-keys";

		vol_dn {
			label = "volume_down";
			gpios = <&pm8994_gpios 2 0x1>;
			linux,input-type = <1>;
			linux,code = <114>;
			/delete-property/ gpio-key,wakeup;
			debounce-interval = <15>;
		};

		vol_up {
			label = "volume_up";
			gpios = <&pm8994_gpios 3 0x1>;
			linux,input-type = <1>;
			linux,code = <115>;
			/delete-property/ gpio-key,wakeup;
			debounce-interval = <15>;
		};

		cam_snapshot {
			label = "cam_snapshot";
			gpios = <&pm8994_gpios 4 0x1>;
			linux,input-type = <1>;
			linux,code = <0x2fe>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};

		cam_focus {
			label = "cam_focus";
			gpios = <&pm8994_gpios 5 0x1>;
			linux,input-type = <1>;
			linux,code = <0x210>;
			gpio-key,wakeup;
			debounce-interval = <15>;
		};
	};

	ldo_vibrator {
		compatible = "ldo-vibrator";
		gpios = <&pmi8994_gpios 1 1>;
	};

	gpio_wakeup {
		compatible = "gpio_wakeup";
		irq_gpio_num = <4>;
		irq_gpios = <9 77 117 125>;
	};

	qcom,msm-thermal {
		qcom,limit-temp = <85>;
		qcom,core-limit-temp = <95>;
        };

	/* UART: BLSP9 */
	uartblsp2uart2ls: serial@075b1000 {
		compatible = "qcom,msm-lsuart-v14";
		reg = <0x75b1000 0x1000>;
		interrupts = <0 115 0>;
		clocks = <&clock_gcc clk_gcc_blsp2_uart3_apps_clk>,
			 <&clock_gcc clk_gcc_blsp2_ahb_clk>;
		clock-names = "core_clk", "iface_clk";
		status = "ok";
		pinctrl-names = "default";
		pinctrl-0 = <&msm_gpio_49 &msm_gpio_50>;
	};

        /* UART: BLSP2 */
	uart@07570000 {
		reg = <0x07570000 0x1000>,
		      <0x7544000 0x2b000>;
		interrupt-map = <0 &intc 0 0 108 0
		                1 &intc 0 0 238 0
		                2 &tlmm 42 0>;
		/delete-property/ qcom,inject-rx-on-wakeup;
		/delete-property/ qcom,rx-char-to-inject;
		qcom,bam-tx-ep-pipe-index = <2>;
		qcom,bam-rx-ep-pipe-index = <3>;
		pinctrl-names = "default", "sleep", "on";
		pinctrl-0 = <&msm_gpio_41_suspend &msm_gpio_42_suspend &msm_gpio_43_suspend &msm_gpio_44_suspend>;
		pinctrl-1 = <&msm_gpio_41_suspend &msm_gpio_42_suspend &msm_gpio_43_suspend &msm_gpio_44_suspend>;
		pinctrl-2 = <&msm_gpio_41_active &msm_gpio_42_active &msm_gpio_43_active &msm_gpio_44_active>;
		status = "okay";
	};

	qcom,cnss {
		status = "disable";
	};

	uart@7571000 {
		compatible = "qcom,msm-hsuart-v14";
		reg = <0x7571000 0x1000>,
		    <0x7544000 0x2b000>;
		interrupt-parent = <&blsp1_uart3>;
		interrupt-map = <0 &intc 0 0 109 0
		                1 &intc 0 0 238 0
		                2 &tlmm 46 0>;
		/delete-property/ qcom,inject-rx-on-wakeup;
		/delete-property/ qcom,rx-char-to-inject;
		pinctrl-names = "default", "sleep", "on";
		pinctrl-0 = <&msm_gpio_45_suspend &msm_gpio_46_suspend &msm_gpio_47_suspend &msm_gpio_48_suspend>;
		pinctrl-1 = <&msm_gpio_45_suspend &msm_gpio_46_suspend &msm_gpio_47_suspend &msm_gpio_48_suspend>;
		pinctrl-2 = <&msm_gpio_45_active &msm_gpio_46_active &msm_gpio_47_active &msm_gpio_48_active>;
		status = "okay";
	};

	bluesleep {
		compatible = "bcm,bluesleep";
		bt_host_wake = <&tlmm 82 0x00>; /* BT_HOST_WAKE */
		bt_ext_wake = <&tlmm 81 0x00>; /* BT_DEV_WAKE */
		interrupt-parent = <&tlmm>;
		interrupts = <82 0>;
		interrupt-names = "host_wake";
		pinctrl-names = "wake_irq_active", "wake_irq_suspend";
		pinctrl-0 = <&msm_gpio_82_active &msm_gpio_81>;
		pinctrl-1 = <&msm_gpio_82_suspend &msm_gpio_81>;
	};

	bcm43xx {
		compatible = "bcm,bcm43xx";
		bcm,reg-on-gpio = <&tlmm 83 0x00>; /* BT_REG_ON */
		pinctrl-names = "default", "sleep";
		pinctrl-0 = <&msm_gpio_83>;
		pinctrl-1 = <&msm_gpio_83>;
	};

