	/* sdc4 */
	sdhc_4: sdhci@7524900 {
		compatible = "qcom,sdhci-msm";
		reg = <0x7524900 0x314>, <0x7524000 0x800>;
		reg-names = "hc_mem", "core_mem";

		interrupts = <0 129 0>, <0 227 0>, <0 228 0>;
		interrupt-names = "hc_irq", "pwr_irq", "sdiowakeup_irq";

		clock-names = "iface_clk", "core_clk";
		clocks = <&clock_gcc clk_gcc_sdcc4_ahb_clk>,
		<&clock_gcc clk_gcc_sdcc4_apps_clk>;

		qcom,large-address-bus;
		qcom,bus-width = <4>;

		qcom,msm-bus,name = "sdhc4";
		qcom,msm-bus,num-cases = <9>;
		qcom,msm-bus,num-paths = <1>;
		qcom,msm-bus,vectors-KBps = <80 512 0 0>, /* No vote */
			<80 512 1600 3200>, /* 400 KB/s*/
			<80 512 80000 160000>, /* 20 MB/s */
			<80 512 100000 200000>, /* 25 MB/s */
			<80 512 200000 400000>, /* 50 MB/s */
			<80 512 400000 800000>, /* 100 MB/s */
			<80 512 400000 800000>, /* 200 MB/s */
			<80 512 400000 800000>, /* 400 MB/s */
			<80 512 2048000 4096000>; /* Max. bandwidth */
		qcom,bus-bw-vectors-bps = <0 400000 20000000 25000000 50000000
			100000000 200000000 400000000 4294967295>;

		qcom,pm-qos-cpu-groups = <0x03 0x0c>;
		qcom,pm-qos-cmdq-latency-us = <70 70>, <70 70>;
		qcom,pm-qos-legacy-latency-us = <70 70>, <70 70>;
		qcom,pm-qos-irq-type = "affine_cores";
		qcom,pm-qos-irq-cpu = <0>;
		qcom,pm-qos-irq-latency = <70 70>;
		status = "ok";
		vdd-io-supply = <&pm8994_s4>;
		qcom,vdd-io-voltage-level = <1800000 1800000>;
		qcom,vdd-io-current-level = <200 15000>;
		qcom,vdd-io-always-on;
		pinctrl-names = "active", "sleep";
		pinctrl-0 = <&sdc4_clk_on &sdc4_cmd_on &sdc4_data0_on &sdc4_data1_on
			&sdc4_data2_on &sdc4_data3_on>;
		pinctrl-1 = <&sdc4_clk_off &sdc4_cmd_off &sdc4_data0_off &sdc4_data1_off
			&sdc4_data2_off &sdc4_data3_off>;
		qcom,clk-rates = <400000 20000000 25000000 50000000 100000000>;
		qcom,bus-speed-mode = "SDR12", "SDR25", "SDR50", "DDR50";
		qcom,nonremovable;
		qcom,core_3_0v_support;
		cd-post;
		wifi-host;
	};

	/* SPI : BLSP12 */
	spi@75BA000{
		status = "okay";
		qcom,use-bam;
		eye0@0 {
			compatible="spidev";
			reg = <0>;
			spi-max-frequency =<38400000>;
		};
	};
	/* SPI : BLSP1 */
	spi@7575000 {
		status = "okay";
		qcom,use-bam;
		pinctrl-0 = <&spi_0_active>;
		pinctrl-1 = <&spi_0_sleep>;
		eye0@0 {
			compatible="spidev";
			reg = <0>;
			spi-max-frequency =<38400000>;
		};
	};

	/* disable tspp */
	msm_tspp@075e7000 {
		status = "disable";
	};
};

